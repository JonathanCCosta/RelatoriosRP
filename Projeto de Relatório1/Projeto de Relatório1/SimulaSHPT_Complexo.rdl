        </QueryParameters>
        <CommandText>SELECT        ID AS COD_COMPLEXO, Sigla
FROM            SPE_E_COMPLEXO
WHERE        (ID IN
                             (SELECT        ID_COMPLEXO
                               FROM            SPE_E_CONTROLE_ACESSO
                               WHERE        (CD_EMPRESA = @empresa)))</CommandText>
      </Query>
