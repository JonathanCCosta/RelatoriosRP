      <Query>
        <DataSourceName>DS_FURNAS_DW</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT DISTINCT 
                         AAA.ID_BALANCO_PATRIMONIAL, AAA.ID_SPE, AAA.ANO, AAA.TRIMESTRE, AAA.ID_ITEM_BP, AAA.ID_NIVEL1_BP, AAA.ID_NIVEL2_BP, AAA.ID_NIVEL3_BP, NIVEIS_BP.ID_NIVEL3_BP AS Expr1, 
                         CAST(AAA.TRIMESTRE AS VARCHAR) + '/' + CAST(AAA.ANO AS VARCHAR) AS DT_REF, AAA.TP_BALANCO_PATRIMONIAL, AAA.VALOR, NIVEIS_BP.TITULO_NIVEL1, NIVEIS_BP.TITULO_NIVEL2, 
                         NIVEIS_BP.TITULO_NIVEL3, NIVEIS_BP.N1_ORDEM, NIVEIS_BP.N2_ORDEM, NIVEIS_BP.N3_ORDEM
FROM            (SELECT        BALANCO_PATRIMONIAL.ID_BALANCO_PATRIMONIAL, BALANCO_PATRIMONIAL.ID_SPE, BALANCO_PATRIMONIAL.ANO, BALANCO_PATRIMONIAL.TRIMESTRE, 
                                                    BALANCO_PATRIMONIAL.TP_BALANCO_PATRIMONIAL, ITEM_BP.ID_BALANCO_PATRIMONIAL AS 'BPiTEM', ITEM_BP.ID_ITEM_BP, ITEM_BP.ID_NIVEL1_BP, 
                                                    CASE WHEN ITEM_BP.[ID_NIVEL2_BP] IS NULL THEN 0 ELSE ITEM_BP.[ID_NIVEL2_BP] END AS ID_NIVEL2_BP, CASE WHEN ITEM_BP.[ID_NIVEL3_BP] IS NULL 
                                                    THEN 0 ELSE ITEM_BP.[ID_NIVEL3_BP] END AS ID_NIVEL3_BP, ITEM_BP.VALOR
                          FROM            sis_spe.BALANCO_PATRIMONIAL AS BALANCO_PATRIMONIAL LEFT OUTER JOIN
                                                    sis_spe.ITEM_BP AS ITEM_BP ON BALANCO_PATRIMONIAL.ID_BALANCO_PATRIMONIAL = ITEM_BP.ID_BALANCO_PATRIMONIAL) AS AAA LEFT OUTER JOIN
                             (SELECT        ID_NIVEL1_BP, TITULO_NIVEL1, 0 AS ID_NIVEL2_BP, NULL AS TITULO_NIVEL2, 0 AS ID_NIVEL3_BP, NULL AS TITULO_NIVEL3, ORDEM AS N1_ORDEM, NULL AS N2_ORDEM, NULL 
                                                         AS N3_ORDEM
                               FROM            sis_spe.NIVEL1_BP AS N1
                               UNION
                               SELECT        N1.ID_NIVEL1_BP, N1.TITULO_NIVEL1, N2.ID_NIVEL2_BP, N2.TITULO_NIVEL2, 0 AS ID_NIVEL3_BP, NULL AS TITULO_NIVEL3, N1.ORDEM AS N1_ORDEM, N2.ORDEM AS N2_ORDEM, NULL 
                                                        AS N3_ORDEM
                               FROM            sis_spe.NIVEL1_BP AS N1 LEFT OUTER JOIN
                                                        sis_spe.NIVEL2_BP AS N2 ON N1.ID_NIVEL1_BP = N2.ID_NIVEL1_BP
                               WHERE        (N2.ID_NIVEL2_BP IS NOT NULL)
                               UNION
                               SELECT        N1.ID_NIVEL1_BP, N1.TITULO_NIVEL1, N2.ID_NIVEL2_BP, N2.TITULO_NIVEL2, N3.ID_NIVEL3_BP, N3.TITULO_NIVEL3, N1.ORDEM AS N1_ORDEM, N2.ORDEM AS N2_ORDEM, 
                                                        N3.ORDEM AS N3_ORDEM
                               FROM            sis_spe.NIVEL1_BP AS N1 LEFT OUTER JOIN
                                                        sis_spe.NIVEL2_BP AS N2 ON N1.ID_NIVEL1_BP = N2.ID_NIVEL1_BP LEFT OUTER JOIN
                                                        sis_spe.NIVEL3_BP AS N3 ON N2.ID_NIVEL2_BP = N3.ID_NIVEL2_BP
                               WHERE        (N2.ID_NIVEL2_BP IS NOT NULL) AND (N3.ID_NIVEL3_BP IS NOT NULL)) AS NIVEIS_BP ON NIVEIS_BP.ID_NIVEL1_BP = AAA.ID_NIVEL1_BP AND NIVEIS_BP.ID_NIVEL2_BP = AAA.ID_NIVEL2_BP AND
                          NIVEIS_BP.ID_NIVEL3_BP = AAA.ID_NIVEL3_BP
ORDER BY AAA.ANO, AAA.TRIMESTRE, NIVEIS_BP.N1_ORDEM, NIVEIS_BP.N2_ORDEM, NIVEIS_BP.N3_ORDEM</CommandText>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
  </DataSets>
